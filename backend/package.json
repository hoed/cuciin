{
    "name": "cuciin-backend",
    "version": "1.0.0",
    "description": "Backend for Cuciin Platform",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn src/index.ts",
        "build": "tsc",
        "postinstall": "prisma generate"
    },
    "prisma": {
