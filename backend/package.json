    },
    "dependencies": {
        "@google/generative-ai": "^0.1.0",
        "@prisma/client": "^5.0.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "socket.io": "^4.7.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.0",
        "prisma": "^5.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.2"
    }
}